{
    "background": {
      "scripts": [
        "background/background.js",
        "background/chromereload.js"
      ]
    },
    "manifest_version": 2,
    "name": "8tracks Media Keys",
    "icons":
   {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
    "description": "",
    "version": "0.1",
    "commands": {
      "next": {
         "description": "Next Song",
         "global": true,
         "suggested_key": {
            "default": "MediaNextTrack"
         }
      },
      "pause": {
         "description": "Play/Pause",
         "global": true,
         "suggested_key": {
            "default": "MediaPlayPause"
         }
      }
   },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "*://*.8tracks.com/*"
    ],
    "options_page": "options/options.html"
}